<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
  xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
  version="2.0">

	<named-query name="TrainingPrj.getData">
		<query>
			<![CDATA[
				SELECT t.cognome FROM TrainingPrj t
			 ]]>
		</query>
	</named-query>
	
	<named-query name="TrainingPrjComune.getData">
		<query>
			<![CDATA[
				SELECT tpc.descrizione FROM TrainingPrjComune tpc order by tpc.descrizione
			 ]]>
		</query>
	</named-query>

</entity-mappings>
