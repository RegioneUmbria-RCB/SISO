<param id\="field" value\="CODICE" dest\="CODICE" type\="java.lang.String" />\
<param id\="field" value\="DESCRIZIONE" dest\="DESCRIZIONE" type\="java.lang.String" />\
<param id\="field" value\="PERCORSO" dest\="PERCORSO" type\="java.lang.String" />\
