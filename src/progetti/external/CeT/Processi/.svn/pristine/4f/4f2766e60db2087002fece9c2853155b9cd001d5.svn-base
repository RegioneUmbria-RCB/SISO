package it.webred.rulengine.brick.elab.cartellaSociale;

class IndirizziZoneDto {

	// chiave logica
	private String zonaSoc;
	private String indirizzoHash;
	//

	/**
	 * dato sorgente
	 */
	private String gmIndirizzoEsteso;

	// dati riportati da gmaps
	private Double latitudine;
	private Double longitudine;
	/**
	 * indirizzo riportato da gmaps
	 */
	private String gmFormattedAddress;
	
	private String toponimo;
	private String numeroCivico;
	private String comune;
	private String siglaProvincia;

	private String geocodeStatus = GeolocalizationResult.UNDEFINED.name();
	private String geocodeStatusDetail = GeolocalizationResult.UNDEFINED.description();

	public String getZonaSoc() {
		return zonaSoc;
	}

	public void setZonaSoc(String zonaSoc) {
		this.zonaSoc = zonaSoc;
	}

	public String getIndirizzoHash() {
		return indirizzoHash;
	}

	public void setIndirizzoHash(String indirizzoHash) {
		this.indirizzoHash = indirizzoHash;
	}

	public String getGmIndirizzoEsteso() {
		return gmIndirizzoEsteso;
	}

	public void setGmIndirizzoEsteso(String gmIndirizzoEsteso) {
		this.gmIndirizzoEsteso = gmIndirizzoEsteso;
	}

	public Double getLatitudine() {
		return latitudine;
	}

	public void setLatitudine(Double latitudine) {
		this.latitudine = latitudine;
	}

	public Double getLongitudine() {
		return longitudine;
	}

	public void setLongitudine(Double longitudine) {
		this.longitudine = longitudine;
	}

	public String getGmFormattedAddress() {
		return gmFormattedAddress;
	}

	public void setGmFormattedAddress(String gmFormattedAddress) {
		this.gmFormattedAddress = gmFormattedAddress;
	}

	public String getGeocodeStatus() {
		return geocodeStatus;
	}

	public void setGeocodeStatus(String geocodeStatus) {
		this.geocodeStatus = geocodeStatus;
	}

	public String getGeocodeStatusDetail() {
		return geocodeStatusDetail;
	}

	public void setGeocodeStatusDetail(String geocodeStatusDetail) {
		this.geocodeStatusDetail = geocodeStatusDetail;
	}
	
	public String getToponimo() {
		return toponimo;
	}

	public void setToponimo(String toponimo) {
		this.toponimo = toponimo;
	}

	public String getNumeroCivico() {
		return numeroCivico;
	}

	public void setNumeroCivico(String numeroCivico) {
		this.numeroCivico = numeroCivico;
	}
	
	public String getComune() {
		return comune;
	}

	public void setComune(String comune) {
		this.comune = comune;
	}

	public String getSiglaProvincia() {
		return siglaProvincia;
	}

	public void setSiglaProvincia(String siglaProvincia) {
		this.siglaProvincia = siglaProvincia;
	}

	@Override
	public String toString() {
		return "IndirizziZoneDto [zonaSoc=" + zonaSoc + ", indirizzoHash=" + indirizzoHash + ", gmIndirizzoEsteso="
				+ gmIndirizzoEsteso + ", latitudine=" + latitudine + ", longitudine=" + longitudine
				+ ", gmFormattedAddress=" + gmFormattedAddress + ", toponimo=" + toponimo + ", numeroCivico="
				+ numeroCivico + ", comune=" + comune + ", siglaProvincia=" + siglaProvincia + ", geocodeStatus="
				+ geocodeStatus + ", geocodeStatusDetail=" + geocodeStatusDetail + "]";
	}	
}
